{
  "name": "google-apis",
  "version": "1.0.0",
  "author": "Benjamin Maggi",
  "description": "Use Google API Discovery Service to build runtime client drivers to interact with Google APIs",
  "homepage": "https://github.com/maggiben/google-apis#readme",
  "bugs": {
    "url": "https://github.com/maggiben/google-apis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maggiben/google-apis.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:typecheck && npm run test:spec",
    "test:lint": "eslint src",
    "test:spec": "cross-env BABEL_ENV=test nyc mocha test/**/*.spec.js",
    "test:typecheck": "flow check",
    "start": "babel-node index.js",
    "build": "npm run build:clean && npm run build:release",
    "build:clean": "rimraf lib",
    "build:release": "babel src --out-dir lib",
    "start:debugger": "cross-env BABEL_ENV=development nodemon --exec node --require babel-register --inspect --debug-brk src/index.js",
    "example:urlshortener": "nodemon --exec node --require babel-register example/urlshortener.js",
    "example:youtube": "nodemon --exec node --require babel-register example/youtube.js",
    "dev": "npm run build:release && node lib/index.js",
    "prepublish": "npm run build:release"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "flow-runtime": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.54.1",
    "mocha": "^3.5.3",
    "nock": "^9.0.14",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1"
  }
}
